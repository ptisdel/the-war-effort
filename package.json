{
  "name": "war-office",
  "version": "1.0.0",
  "description": "A warmaking cooperative party game for 4-8 players.",
  "scripts": {
    "commit": "npx git-cz",
    "dev": "lerna run dev",
    "dev:test": "lerna run dev:test",
    "lint": "lerna run lint",
    "e2e": "start-server-and-test dev:test \"http://localhost:8001|http://localhost:8002\" \"lerna run cypress:run\"",
    "e2e:interactive": "start-server-and-test dev:test \"http://localhost:8001|http://localhost:8002\" \"lerna run cypress:open\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ptisdel/war-office.git"
  },
  "author": "Patrick Tisdel",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ptisdel/war-office/issues"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "homepage": "https://github.com/ptisdel/war-office#readme",
  "devDependencies": {
    "@babel/eslint-parser": "7.18.2",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.26.0",
    "lerna": "5.1.4",
    "start-server-and-test": "^1.14.0",
    "wait-on": "6.0.1"
  }
}